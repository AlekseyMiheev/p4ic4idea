<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="net.groboclown.p4plugin.ui.vcsroot.P4RootConfigPanel">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="9051d" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <nested-form id="45f11" form-file="net/groboclown/p4plugin/ui/vcsroot/ConfigPartStack.form" binding="myConfigPartStack" custom-create="true">
            <constraints border-constraint="Center"/>
          </nested-form>
        </children>
      </grid>
      <grid id="e0cb0" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="6c9f8" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="6a442" class="javax.swing.JButton" binding="myCheckConnectionButton">
                <constraints border-constraint="East"/>
                <properties>
                  <text resource-bundle="net/groboclown/p4plugin/P4Bundle" key="configuration.check-connection.button"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="4e649" binding="myConfigRefreshDetailsPanel" custom-create="true" layout-manager="FormLayout">
            <rowspec value="center:d:noGrow"/>
            <rowspec value="top:4dlu:noGrow"/>
            <rowspec value="center:max(d;4px):noGrow"/>
            <colspec value="fill:d:grow"/>
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <grid id="cb3eb" binding="myProblemsPanel" layout-manager="FormLayout">
                <rowspec value="center:d:grow"/>
                <rowspec value="top:4dlu:noGrow"/>
                <rowspec value="center:d:grow"/>
                <colspec value="fill:d:grow"/>
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                  <forms/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <scrollpane id="23e7b">
                    <constraints>
                      <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                      <forms defaultalign-horz="false" defaultalign-vert="false"/>
                    </constraints>
                    <properties/>
                    <border type="none"/>
                    <children>
                      <component id="a2e38" class="javax.swing.JList" binding="myProblemsList">
                        <constraints/>
                        <properties/>
                      </component>
                    </children>
                  </scrollpane>
                </children>
              </grid>
              <grid id="255bb" layout-manager="FormLayout">
                <rowspec value="center:d:grow"/>
                <rowspec value="top:4dlu:noGrow"/>
                <rowspec value="center:d:grow"/>
                <colspec value="fill:d:grow"/>
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                  <forms/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <scrollpane id="afc19">
                    <constraints>
                      <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                      <forms defaultalign-horz="false" defaultalign-vert="false"/>
                    </constraints>
                    <properties/>
                    <border type="none"/>
                    <children>
                      <component id="3bbf8" class="javax.swing.JTextPane" binding="myResolvedProperties">
                        <constraints/>
                        <properties>
                          <editable value="false"/>
                          <font name="DialogInput"/>
                          <toolTipText resource-bundle="net/groboclown/p4plugin/P4Bundle" key="configuration.resolved.tooltip"/>
                        </properties>
                      </component>
                    </children>
                  </scrollpane>
                </children>
              </grid>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
